function out = calcSmoothTrap(x)
    for i=1:length(x)
        if x(i)<4
            y(i) = smf(x(i),[0,4]);
        elseif x(i)>=4 && x(i)<6
            y(i) = 1;
        else
            y(i) = zmf(x(i),[6,10]);
        end;
    end;
    out = y;
end;

function out = calcImplication(x1,x2)
x1 = linspace(0,15,1000);
x2 = linspace(0,10);
[X,Y] = meshgrid(x1,x2);

z1 = trimf(X,[7,9,13]);
z2 = calcSmoothTrap(x2);
plot(x2,z2);
ylim([-0.05 1.05])
end;